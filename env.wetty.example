# Wetty Production Environment Variables

# Wetty Configuration
WETTY_PORT=3000
WETTY_BASE_PATH=/terminal/
WETTY_TITLE=DataSphere SSH Terminal

# Security
WETTY_FORCE_SSH=true
WETTY_ALLOW_IFRAME=true

# Session Configuration
WETTY_SESSION_TIMEOUT=3600000

# Logging
WETTY_LOG_LEVEL=info

# SSL/TLS (for production)
# WETTY_SSL_KEY=/path/to/ssl/key.pem
# WETTY_SSL_CERT=/path/to/ssl/cert.pem

# Nginx Configuration
NGINX_PORT_HTTP=80
NGINX_PORT_HTTPS=443

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_BURST=20
